// test_func.ailang - Simple test to verify function calls work

Function.Test.Simple {
    Input: x: Integer
    Output: Integer
    Body: {
        PrintMessage("Inside Test.Simple, x = ")
        PrintNumber(x)
        PrintMessage("")
        result = Add(x, 10)
        ReturnValue(result)
    }
}

SubRoutine.Main {
    PrintMessage("Testing function call...")
    
    result = Test.Simple(5)
    
    PrintMessage("Result: ")
    PrintNumber(result)
    PrintMessage("")
    
    PrintMessage("Test complete")
}

RunTask(Main)