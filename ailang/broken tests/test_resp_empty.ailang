// test_resp_empty.ailang  
// Test empty array specifically

LibraryImport.Library.RESP

PrintMessage("Testing empty array...")
arr = ArrayCreate(0)
PrintMessage("Empty array created")

response = RESP.Array(arr)
PrintMessage("RESP.Array called")
PrintMessage("Response: ")
PrintMessage(response)

ArrayDestroy(arr)
Deallocate(response, 0)
PrintMessage("Done")