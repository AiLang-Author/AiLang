// Test tracking variables
total_tests = 0
passed_tests = 0
failed_tests = 0
test_name = ""
expected = 0
actual = 0

// Test result helper
SubRoutine.TestResult {
    total_tests = Add(total_tests, 1)
    
    PrintMessage("  TEST: ")
    PrintMessage(test_name)
    PrintMessage("    Expected: ")
    PrintNumber(expected)
    PrintMessage("    Actual: ")
    PrintNumber(actual)
    
    IfCondition EqualTo(expected, actual) ThenBlock: {
        PrintMessage("    âœ… PASS")
        passed_tests = Add(passed_tests, 1)
    } ElseBlock: {
        PrintMessage("    âŒ FAIL")
        failed_tests = Add(failed_tests, 1)
    }
    PrintMessage("")
}

// Summary helper
SubRoutine.PrintSummary {
    PrintMessage("====================================================")
    PrintMessage("ğŸ“Š TEST SUMMARY")
    PrintMessage("====================================================")
    PrintMessage("Total Tests: ")
    PrintNumber(total_tests)
    PrintMessage("Passed: ")
    PrintNumber(passed_tests)
    PrintMessage("Failed: ")
    PrintNumber(failed_tests)
    
    IfCondition EqualTo(failed_tests, 0) ThenBlock: {
        PrintMessage("")
        PrintMessage("ğŸ‰ ALL TESTS PASSED! AIMacro library is working correctly.")
    } ElseBlock: {
        PrintMessage("")
        PrintMessage("âš ï¸  Some tests failed. Check implementation.")
    }
    PrintMessage("====================================================")
}

