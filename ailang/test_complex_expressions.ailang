// test_complex_expressions.ailang
// Test nested and complex expressions

PrintMessage("Complex Expressions Test")
PrintMessage("=========================")

// Test nested arithmetic
result = Add(Multiply(3, 4), Subtract(10, 5))
PrintMessage("(3 * 4) + (10 - 5) = 17:")
PrintNumber(result)

// Test complex boolean logic
a = 5
b = 10
c = 15
cond1 = LessThan(a, b)
cond2 = GreaterThan(c, b)
both = BitwiseAnd(cond1, cond2)
PrintMessage("(5 < 10) AND (15 > 10) = 1:")
PrintNumber(both)

// Test deeply nested function calls
Function.Test.Add1 {
    Input: x:
    Body: {
        ReturnValue(Add(x, 1))
    }
}

nested = Test.Add1(Test.Add1(Test.Add1(Test.Add1(0))))
PrintMessage("Add1(Add1(Add1(Add1(0)))) = 4:")
PrintNumber(nested)

PrintMessage("Complex expressions test complete!")