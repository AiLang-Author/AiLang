// test_byte_access_fixed.ailang
PrintMessage("Byte Access Test - Fixed")
buffer = Allocate(64)

// Store as bytes
StoreValue(buffer, 42, "byte")
StoreValue(Add(buffer, 1), 71, "byte")

// Read as bytes - use quoted strings
val1 = Dereference(buffer, "byte")
val2 = Dereference(Add(buffer, 1), "byte")

PrintMessage("Values:")
PrintNumber(val1)
PrintNumber(val2)